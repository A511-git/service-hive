				{
					"name": "login",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// ===== ACCESS TOKEN (from Authorization header) =====\r",
									"const authHeader =\r",
									"    pm.response.headers.get(\"Authorization\") ||\r",
									"    pm.response.headers.get(\"authorization\");\r",
									"\r",
									"if (authHeader && authHeader.startsWith(\"Bearer \")) {\r",
									"    const accessToken = authHeader.replace(\"Bearer \", \"\");\r",
									"    pm.collectionVariables.set(\"ACCESS_TOKEN\", accessToken);\r",
									"}\r",
									"\r",
									"// ===== REFRESH TOKEN (from cookies) =====\r",
									"const cookies = pm.cookies.toObject();\r",
									"\r",
									"if (cookies.refreshToken) {\r",
									"    pm.collectionVariables.set(\"REFRESH_TOKEN\", cookies.refreshToken);\r",
									"}\r",
									"\r",
									"\r",
									"pm.test('Successful login returns status 200', function () {\r",
									"    pm.response.to.have.status(200);\r",
									"})\r",
									"\r",
									"pm.test('Response time is less than 2000ms', function () {\r",
									"    pm.expect(pm.response.responseTime).to.be.below(2000);\r",
									"})\r",
									"\r",
									"pm.test('Content-Type is application/json', function () {\r",
									"    pm.expect(pm.response.headers.get('Content-Type')).to.include('application/json');\r",
									"})\r",
									"\r",
									"pm.test('Response contains access token', function () {\r",
									"    const jsonData = pm.response.json();\r",
									"    pm.expect(jsonData).to.be.an('object');\r",
									"    const hasAccessToken = jsonData.accessToken || jsonData.access_token || jsonData.token;\r",
									"    pm.expect(hasAccessToken).to.exist;\r",
									"})\r",
									"\r",
									"pm.test('Extract and store ACCESS_TOKEN', function () {\r",
									"    const jsonData = pm.response.json();\r",
									"    const accessToken = jsonData.accessToken || jsonData.access_token || jsonData.token;\r",
									"    if (accessToken) {\r",
									"        pm.collectionVariables.set('ACCESS_TOKEN', accessToken);\r",
									"        pm.expect(accessToken).to.be.a('string');\r",
									"        pm.expect(accessToken.length).to.be.greaterThan(0);\r",
									"    }\r",
									"})\r",
									"\r",
									"pm.test('Extract and store REFRESH_TOKEN if provided', function () {\r",
									"    const jsonData = pm.response.json();\r",
									"    const refreshToken = jsonData.refreshToken || jsonData.refresh_token;\r",
									"    if (refreshToken) {\r",
									"        pm.collectionVariables.set('REFRESH_TOKEN', refreshToken);\r",
									"        pm.expect(refreshToken).to.be.a('string');\r",
									"        pm.expect(refreshToken.length).to.be.greaterThan(0);\r",
									"    }\r",
									"})\r",
									"\r",
									"pm.test('Response contains user information', function () {\r",
									"    const jsonData = pm.response.json();\r",
									"    if (jsonData.user) {\r",
									"        pm.expect(jsonData.user).to.be.an('object');\r",
									"        if (jsonData.user.id)\r",
									"            pm.expect(jsonData.user.id).to.exist;\r",
									"        if (jsonData.user.email)\r",
									"            pm.expect(jsonData.user.email).to.be.a('string');\r",
									"    }\r",
									"})\r",
									"\r",
									"pm.test('Returns 401 for invalid credentials', function () {\r",
									"    if (pm.response.code === 401) {\r",
									"        const jsonData = pm.response.json();\r",
									"        pm.expect(jsonData).to.have.any.keys('error', 'message');\r",
									"    }\r",
									"})\r",
									"\r",
									"pm.test('Returns 400 for missing required fields', function () {\r",
									"    if (pm.response.code === 400) {\r",
									"        const jsonData = pm.response.json();\r",
									"        pm.expect(jsonData).to.have.any.keys('error', 'message', 'errors');\r",
									"    }\r",
									"})\r",
									"\r",
									"pm.test('Returns 404 for non-existent user', function () {\r",
									"    if (pm.response.code === 404) {\r",
									"        const jsonData = pm.response.json();\r",
									"        pm.expect(jsonData).to.have.any.keys('error', 'message');\r",
									"    }\r",
									"})"
								],
								"type": "text/javascript",
